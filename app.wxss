/* 导入图标 */
@import "./styles/iconfont.wxss";


/* 在微信小程序中不支持通配符‘*’ 消除边距，初始化页面样式*/
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 主题颜色 通过变量来实现
1 less 中存在 变量这个知识
2 原生的css和wxss也是支持 变量 */
page{
  /* 定义主题颜色 */
  --themeColor:#fdd75a;
  /* 同一字体大小 */
  font-size: 28rpx;
}